#:kivy 1.0.9

<Settings>:    
    orientation: 'vertical'
    BoxLayout:
        orientation: 'vertical'
        BoxLayout:
            orientation: 'horizontal'
            Label:
                text: root.executablePath
            Button:
                text: 'Choose Executable'
                on_release: root.chooseExecutable()
        BoxLayout:
            orientation: 'horizontal'
            Label:
                text: root.configPath
            Button:
                text: 'Choose Config'
                on_release: root.chooseConfig()
